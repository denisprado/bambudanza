{
  "parser": "babel-eslint",
  "plugins": ["react"],
  "rules": {
    "strict": 0,
    "react/prop-types": "off",
    "react/jsx-uses-react": "off",
    "react/jsx-uses-vars": "off",
    "react/display-name": "warm",
    "no-unused-vars": "warn",
    "no-undef": "warn"
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"]
}
